{
  "name": "@mrgalaxy/tjme",
  "version": "0.0.0-edge",
  "description": "My personal website.",
  "author": "Tyler Johnson <tyler@tylerjohnson.me>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tyler-johnson/tylerjohnson.me.git"
  },
  "main": "index.js",
  "bin": {
    "tjme": "cli.js"
  },
  "scripts": {
    "lint": "eslint src/ test/",
    "clean": "shx rm -rf cli.js index.js styles.css",
    "build-deploy": "node .deploy/build",
    "deploy": "npm run build-deploy && kubectl apply -f kubernetes.yml",
    "build-lib": "rollup src/index.js -c > index.js",
    "build-cli": "shx echo \"#!/usr/bin/env node\" > cli.js && rollup src/cli.js -c >> cli.js && shx chmod +x cli.js",
    "build-styles": "node-sass src/styles.scss | cssnano > styles.css",
    "build": "npm-run-all clean build-lib build-cli build-styles",
    "autorelease": "autorelease pre && npm publish && autorelease post"
  },
  "dependencies": {
    "es6-promisify": "^5.0.0",
    "express": "^4.13.4",
    "lodash": "^4.5.1",
    "minimist": "^1.2.0",
    "nunjucks": "^3.0.0",
    "rc": "^1.2.1",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "autorelease": "^1.5.1",
    "autorelease-github": "^1.1.1",
    "autorelease-travis": "^1.2.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "clean-css": "^4.0.12",
    "cssnano": "^3.10.0",
    "cssnano-cli": "^1.0.5",
    "eslint": "^3.19.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "postcss-cli": "^3.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-json": "^2.1.1",
    "shx": "^0.2.2"
  },
  "keywords": [
    "tyler",
    "johnson",
    "tylerjohnson.me",
    "tjme"
  ],
  "license": "MIT",
  "files": [
    "index.js",
    "cli.js",
    "styles.css",
    "views"
  ]
}
